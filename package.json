{
  "name": "setup-actions-toolkit",
  "version": "0.0.0",
  "private": true,
  "description": "Install Bash implementation of github-core commands",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "inspect": "npm run lint && npm run format-check && npm run test",
    "package": "rm -fr lib dist; npm run build && ncc build --source-map --license licenses.txt",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmatsu/setup-actions-toolkit.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Jumpei Matsuda",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.15.6",
    "@typescript-eslint/parser": "^5.50.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.33.0",
    "eslint-plugin-github": "^4.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.4.1",
    "jest-circus": "^29.4.1",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/tool-cache": "^2.0.1"
  }
}
