{
  "name": "setup-actions-toolkit",
  "version": "0.0.0",
  "private": true,
  "description": "Install Bash implementation of github-core commands",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "inspect": "npm run lint && npm run format-check && npm run test",
    "package": "rm -fr lib dist; npm run build && ncc build --source-map --license licenses.txt",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmatsu/setup-actions-toolkit.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Jumpei Matsuda",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/tool-cache": "^1.6.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.9",
    "@typescript-eslint/parser": "^4.31.0",
    "@vercel/ncc": "^0.36.0",
    "eslint": "^7.23.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.3.4",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^3.14.0",
    "prettier": "2.2.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
